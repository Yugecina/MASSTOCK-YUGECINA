{
  "name": "masstock-backend",
  "version": "1.0.0",
  "description": "MasStock SaaS Backend - Workflow automation for AI content agencies",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon src/server.ts",
    "test": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=html --coverageReporters=text",
    "test:ci": "jest --coverage --ci --reporters=default",
    "test:changed": "jest --onlyChanged",
    "test:file": "jest --findRelatedTests",
    "migrate": "ts-node src/database/migrate.ts",
    "worker": "ts-node src/workers/workflow-worker.ts"
  },
  "keywords": [
    "saas",
    "workflow",
    "automation",
    "ai"
  ],
  "author": "MasStock",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bull": "^4.12.0",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "p-limit": "^3.1.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "yamljs": "^0.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "axios-cookiejar-support": "^6.0.4",
    "ioredis-mock": "^8.13.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "tough-cookie": "^6.0.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
